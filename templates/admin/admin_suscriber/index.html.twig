{% extends 'admin/base.html.twig' %}

{% block title %}FAQ{% endblock %}

{% block body %}
    <h1 class="text-center">Liste des Abonnés à la newsletter</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
        {% for suscriber in suscribers %}
            <tr>
                <td>{{ suscriber.email }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}